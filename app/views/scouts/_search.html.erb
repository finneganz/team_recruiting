<div class="">
    
    <div id="search_bar">
    <%= search_form_for @q, url: root_path do |f| %>
    <div class="col-md-3 col-md-offset-3">
        <%= f.label :mm_rank, 'MMのランク' %>
        <%= f.search_field :mm_rank_cont %>
    </div>
    <div class="col-md-3 col-md-offset-6>
        <%= f.label :pugs_elo, 'PUGのELO' %>
        <%= f.number_field :pugs_elo_gteq %>以上〜
        <%= f.number_field :pugs_elo_lt %>未満<br />
    </div>
    <div class="col-md-3 col-md-offset-3">
        <%= f.label :active_time, '活動時間' %>
        <%= f.search_field :active_time_cont %>
    </div>
    <div class="col-md-3 col-md-offset-3">
        <%= f.label :active_week, '1週間のイン率' %>
        <%= f.search_field :active_week_cont %>
    </div>
    <div class="col-md-3 col-md-offset-3">
        <%= f.label :playstyle, 'プレイスタイル' %>
        <%= f.search_field :playstyle_cont %>
    </div>
    <div class="col-md-3 col-md-offset-3">
        <%= f.label :role, 'ゲーム内のROLE' %>
        <%= f.search_field :role_cont %>
    </div>
    <div class="col-md-3 col-md-offset-1">
        <%= f.submit '検索する' %>
    </div>
    <% end %>
    
    </div>
</div>